{
  "name": "updose",
  "version": "0.1.0",
  "type": "module",
  "description": "AI coding tool boilerplate marketplace",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "bin": {
    "updose": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit && biome check src",
    "lint:fix": "tsc --noEmit && biome check --write src",
    "format": "biome format --write src",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alchemist85K/updose.git"
  },
  "keywords": [
    "ai",
    "boilerplate",
    "claude",
    "codex",
    "gemini",
    "cli"
  ],
  "author": "Alchemist85K",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "bugs": {
    "url": "https://github.com/Alchemist85K/updose/issues"
  },
  "homepage": "https://github.com/Alchemist85K/updose#readme",
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@types/node": "^25.3.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "prompts": "^2.4.2"
  }
}
